    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "leaflet": "^1.9.4",
    "moment": "^2.29.4",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "eslint": "^8.10.0",
    "eslint-plugin-vue": "^9.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "eslint-config-prettier": "^8.1.0",
    "prettier": "^2.5.1",
    "@quasar/app-webpack": "^3.0.0"
  },
  "engines": {
    "node": ">= 14.21.3",
    "npm": ">= 9.5.1",
    "yarn": ">= 3.5.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
