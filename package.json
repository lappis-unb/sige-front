{
  "name": "smi-front",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "SIGE - Sistema de Gestão Energética",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "author": "",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "test": "yarn test:unit:ci",
    "cl": "auto-changelog",
    "start": "quasar dev -m ssr",
    "build": "quasar build",
    "build:ssr": "quasar build -m ssr",
    "start:ssr": "node dist/ssr/index.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@quasar/extras": "^1.0.0",
    "apexcharts": "^3.25.0",
    "axios": "^0.19.2",
    "leaflet": "^1.6.0",
    "moment": "^2.25.2",
    "quasar": "^1.0.0",
    "vue-apexcharts": "^1.5.3",
    "vue2-leaflet": "^2.5.2",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@quasar/app": "^1.0.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^2.2.3",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/vue": "^6.4.19",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.2.3",
    "core-js": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "vue-loader": "^15.9.8"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "@babel/parser": "7.7.5"
  }
}
