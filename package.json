    "prettier": "^2.8.8",
    "vue-loader": "^17.1.1"
  },
  "engines": {
    "node": ">= 16.18.1",
    "npm": ">= 9.5.1",
    "yarn": ">= 3.5.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
