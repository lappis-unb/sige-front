    "axios": "^0.21.1",
    "leaflet": "^1.9.4",
    "moment": "^2.29.4",
    "quasar": "^2.12.0",
    "vue-apexcharts": "^1.6.2",
    "vue2-leaflet": "^2.7.1",
    "webpack": "^5.84.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
